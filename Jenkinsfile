    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                echo 'Build started...'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
                echo 'Deployed.'
            }
        }
    }
}